{
  "manifest_version": 3,
  "name": "GitHub Lines Viewed",
  "version": "1.0.2",
  "description": "Replaces GitHub's 'files viewed' with 'lines viewed' instead.",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "js": ["lines-viewed.js"]
    }
  ],
  "permissions": ["activeTab"]
}
