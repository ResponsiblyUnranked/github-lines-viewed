{
  "manifest_version": 2,
  "name": "GitHub Lines Viewed",
  "version": "1.0.2",
  "description": "Replaces GitHub's 'files viewed' with 'lines viewed' instead.",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "js": ["lines-viewed.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{1f27e2d0-740c-46eb-978a-9c119bc60371}"
    }
  }
}
